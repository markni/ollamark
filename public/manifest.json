{
  "manifest_version": 3,
  "name": "Ollamark",
  "version": "1.0.1850",
  "description": "Ollama Powered AI Bookmark Sortor",
  "permissions": [
    "bookmarks",
    "tabs",
    "windows",
    "declarativeNetRequest"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {},
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}