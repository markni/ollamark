{
  "manifest_version": 3,
  "name": "Ollamark",
  "version": "1.0.2018",
  "description": "Ollama Powered AI Bookmark Sortor",
  "icons": {
    "16": "icons/ollamark-logo-16.png",
    "48": "icons/ollamark-logo-48.png",
    "128": "icons/ollamark-logo-128.png"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "windows",
    "declarativeNetRequest"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {},
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "host_permissions": [
    "http://localhost/*"
  ]
}